{
  "name": "pollen-solutions/form-mailer",
  "description": "Pollen Form Mailer Component.",
  "homepage": "https://www.presstify.com/pollen-solutions/form-mailer/",
  "keywords": [
    "pollen-solutions",
    "component",
    "forms",
    "mailer"
  ],
  "time": "2021-02-23",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/form": "^1.0",
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\FormMailer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\FormMailer\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
